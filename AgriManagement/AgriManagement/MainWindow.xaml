                    <ComboBox x:Name="txt_BW" HorizontalAlignment="Left" Height="23" Margin="150,482,0,0" Width="107" VerticalAlignment="Top"></ComboBox>
                    <ComboBox x:Name="txt_rate" HorizontalAlignment="Left" Height="23" Margin="150,522,0,0" Width="107" VerticalAlignment="Top"></ComboBox>
