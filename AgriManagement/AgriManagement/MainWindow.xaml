                    <TextBox x:Name="txt_RH_min" HorizontalAlignment="Left" Height="23" Margin="769,113,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
                    <TextBox x:Name="txt_RH_deviation" HorizontalAlignment="Left" Height="23" Margin="969,113,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="60"/>
                    <Button x:Name="btn_updateSetting" Content="确认" HorizontalAlignment="Left" Margin="428,172,0,0" VerticalAlignment="Top" Width="75" Click="btn_updateSetting_Click"/>
                    <CheckBox x:Name="cb_play" Content="异常播放音乐" HorizontalAlignment="Left" Margin="552,172,0,0" VerticalAlignment="Top"/>

                    <CheckBox x:Name="cb_temp" Content="是否显示温度" HorizontalAlignment="Left" Margin="430,220,0,0" VerticalAlignment="Top"/>
                    <CheckBox x:Name="cb_moist" Content="是否显示湿度" HorizontalAlignment="Left" Margin="430,240,0,0" VerticalAlignment="Top"/>
                    <CheckBox x:Name="cb_ch3" Content="是否显示氨气" HorizontalAlignment="Left" Margin="430,260,0,0" VerticalAlignment="Top"/>
                    <CheckBox x:Name="cb_data1" Content="是否显示土壤温度" HorizontalAlignment="Left" Margin="430,300,0,0" VerticalAlignment="Top"/>
                    <CheckBox x:Name="cb_data2" Content="是否显示土壤湿度" HorizontalAlignment="Left" Margin="430,320,0,0" VerticalAlignment="Top"/>
                    <CheckBox x:Name="cb_data3" Content="是否显示光强" HorizontalAlignment="Left" Margin="430,340,0,0" VerticalAlignment="Top"/>

                    <DataGrid Margin="50,67,0,0" AutoGenerateColumns="False" HorizontalAlignment="Left" VerticalAlignment="Top" Name="userData" IsReadOnly="True">
                        <DataGrid.Columns>
                            <DataGridTextColumn Header="ID" Width="100" Binding="{Binding id}"/>
                            <DataGridTextColumn Header="名称" Width="100" Binding="{Binding name}"/>
                            <DataGridTemplateColumn Header="编辑">
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <Button Name="btn_edt" Click="btn_edt_Click">编辑</Button>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn Header="删除">
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <Button Name="btn_del" Click="btn_del_Click">删除</Button>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                        </DataGrid.Columns>
                    </DataGrid>

                    <Button x:Name="btn_adduser" Content="添加用户" HorizontalAlignment="Left" Margin="158,21,0,0" VerticalAlignment="Top" Width="75" Click="btn_adduser_Click"/>
                    <Button x:Name="btn_showAllUsers" Content="显示全部用户" HorizontalAlignment="Left" Margin="50,20,0,0" VerticalAlignment="Top" Width="75" Click="btn_showAllUsers_Click"/>



                </Grid>
            </TabItem>
            <TabItem Name="app_cmd" Header="调试接口" BorderBrush="#FF232020">
                <Grid Background="#FFE5E5E5">
                    <Label x:Name="label9" Content="服务器地址：" HorizontalAlignment="Left" Margin="50,50,0,0" VerticalAlignment="Top"/>
