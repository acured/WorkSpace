<Window x:Class="AgriManagement.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AgriManagement"
        mc:Ignorable="d"
        Title="监控系统电脑控制端" Height="820" Width="1080" Closed="Window_Closed">
    <Grid>
        <TabControl x:Name="tabControl" HorizontalAlignment="Left" Height="760" Margin="10,10,0,0" VerticalAlignment="Top" Width="1052">
            <TabItem Header="实时信息">
                <Grid Background="#FFE5E5E5">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="459*"/>
                        <ColumnDefinition Width="587*"/>
                    </Grid.ColumnDefinitions>
                    <CheckBox x:Name="cb_update" HorizontalAlignment="Left" Margin="440,20,0,0" VerticalAlignment="Top" Content="开启云同步" Grid.Column="1"/>
                    <!--<TextBox x:Name="txt_port" HorizontalAlignment="Left" Height="23" Margin="415,21,0,0" TextWrapping="Wrap" Text="COM3" VerticalAlignment="Top" Width="71" Grid.Column="1"/>-->
                    <!--<Button x:Name="btn_start" Content="启动" HorizontalAlignment="Left" Margin="491,21,0,0" VerticalAlignment="Top" Width="75" Click="btn_start_Click" Grid.Column="1"/>-->
                    <ScrollViewer Name="scrollViewer1" VerticalScrollBarVisibility="Auto" Margin="10,0,200,10" Grid.ColumnSpan="2">
                        <Canvas Name="cav_show" Width="800" Height="600" HorizontalAlignment="Left" VerticalAlignment="Top" />
                    </ScrollViewer>
                    <CheckBox x:Name="cb_1" Content="" HorizontalAlignment="Left" Margin="440,84,0,0" VerticalAlignment="Top" Checked="cb_1_Checked" Unchecked="cb_1_Unchecked" Grid.Column="1"/>
                    <CheckBox x:Name="cb_2" Content="" HorizontalAlignment="Left" Margin="440,104,0,0" VerticalAlignment="Top" Checked="cb_1_Checked" Unchecked="cb_1_Unchecked" Grid.Column="1"/>
                    <CheckBox x:Name="cb_3" Content="" HorizontalAlignment="Left" Margin="440,124,0,0" VerticalAlignment="Top" Checked="cb_1_Checked" Unchecked="cb_1_Unchecked" Grid.Column="1"/>
                    <CheckBox x:Name="cb_4" Content="" HorizontalAlignment="Left" Margin="440,144,0,0" VerticalAlignment="Top" Checked="cb_1_Checked" Unchecked="cb_1_Unchecked" Grid.Column="1"/>
                </Grid>
            </TabItem>
            <TabItem Header="趋势展示" Margin="-2,0,-2,-2">
                <Grid Background="#FFE5E5E5">
                    <ComboBox x:Name="cmb_areachat" Margin="10,10,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Width="120" SelectionChanged="cmb_areachat_SelectionChanged"/>
                    <ComboBox x:Name="cmb_nodechat" Margin="150,10,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Width="120" SelectionChanged="cmb_nodechat_SelectionChanged"/>
                    <Canvas Name="cav_chat" Width="700" Height="740" Grid.ColumnSpan="2" Margin="23,10,23,11" HorizontalAlignment="Left" VerticalAlignment="Top">
                    </Canvas>
                    <ScrollViewer Grid.Column="1" Name="scrollViewer3" VerticalScrollBarVisibility="Auto" Margin="880,50,0,0">
                        <TextBlock Name="txt_error_facility" Panel.ZIndex="99" Width="200" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,0,0,0" />
                    </ScrollViewer>
                </Grid>
            </TabItem>
            <TabItem Header="历史查询">
                <Grid Background="#FFE5E5E5">
                    <ScrollViewer Grid.Column="1" Name="scrollViewer4" VerticalScrollBarVisibility="Auto" Margin="15,10,23,11">
                        <Canvas Name="cav_history" Width="1020" Height="3000" Grid.ColumnSpan="2" Margin="0,0,0,0"/>
                    </ScrollViewer>
                </Grid>
            </TabItem>
            <TabItem Header="历史数据">
                <Grid  HorizontalAlignment="Stretch" Name="grid1" VerticalAlignment="Stretch">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="139*"/>
                        <ColumnDefinition Width="384*"/>
                    </Grid.ColumnDefinitions>
                    <ComboBox x:Name="cmb_areatable" Margin="10,10,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Width="120" SelectionChanged="cmb_areatable_SelectionChanged"/>
                    <ComboBox x:Name="cmb_nodetable" Margin="150,10,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Width="120"/>
                    <DataGrid Margin="72,50" AutoGenerateColumns="False" HorizontalAlignment="Center" VerticalAlignment="Stretch" Name="SrollBarDataGrid" IsReadOnly="True" Grid.ColumnSpan="2">
                        <DataGrid.Resources>
                            <Style TargetType="DataGridRow">
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding status}" Value="Error">
                                        <Setter Property="Background" Value="Red"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding status}" Value="Warning">
                                        <Setter Property="Background" Value="Yellow"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding isShow}" Value="false">
                                        <Setter Property="Visibility" Value="Hidden"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                            <Style TargetType="{x:Type DataGridCell}">
                                <Style.Triggers>
                                    <Trigger Property="DataGridCell.IsSelected" Value="True">
                                        <Setter Property="Background" Value="#CCDAFF" />
                                    </Trigger>
                                </Style.Triggers>
                            </Style>
                        </DataGrid.Resources>
                        <DataGrid.Columns>
                            <DataGridTextColumn Header="设备ID" Width="100" Binding="{Binding id}"/>
                            <DataGridTextColumn Header="设备名称" Width="100" Binding="{Binding name}"/>
                            <DataGridTextColumn Header="网络区域" Width="100" Binding="{Binding area}"/>
                            <DataGridTextColumn Header="湿度" Width="100" Binding="{Binding moisture}"/>
                            <!--<DataGridTextColumn Header="氨气浓度" Width="100" Binding="{Binding NH}"/>-->
                            <DataGridTextColumn Header="温度" Width="100" Binding="{Binding temperature}"/>
                            <DataGridTextColumn Header="时间" Width="200" Binding="{Binding time}"/>
                            <DataGridTextColumn Header="状态" Width="100" Binding="{Binding status}"/>
                        </DataGrid.Columns>
                    </DataGrid>
                    <Button x:Name="btn_search" Content="查询" Grid.Column="1" HorizontalAlignment="Left" Margin="36,12,0,0" VerticalAlignment="Top" Width="75" Click="btn_search_Click"/>
                </Grid>
            </TabItem>
            <TabItem Header="报警管理">
                <Grid Background="#FFE5E5E5">
                    <DataGrid Margin="0,50" AutoGenerateColumns="False" HorizontalAlignment="Center" VerticalAlignment="Stretch" Name="errorData" IsReadOnly="True">
                        <DataGrid.Resources>
                            <Style TargetType="DataGridRow">
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding status}" Value="Error">
                                        <Setter Property="Background" Value="Red"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                            <Style TargetType="{x:Type DataGridCell}">
                                <Style.Triggers>
                                    <Trigger Property="DataGridCell.IsSelected" Value="True">
                                        <Setter Property="Background" Value="#CCDAFF" />
                                    </Trigger>
                                </Style.Triggers>
                            </Style>
                        </DataGrid.Resources>
                        <DataGrid.Columns>
                            <DataGridTextColumn Header="ID" Width="100" Binding="{Binding id}"/>
                            <DataGridTextColumn Header="设备名称" Width="100" Binding="{Binding name}"/>
                            <DataGridTextColumn Header="网络区域" Width="100" Binding="{Binding area}"/>
                            <DataGridTextColumn Header="湿度" Width="100" Binding="{Binding moisture}"/>
                            <!--<DataGridTextColumn Header="氨气浓度" Width="100" Binding="{Binding NH}"/>-->
                            <DataGridTextColumn Header="温度" Width="100" Binding="{Binding temperature}"/>
                            <DataGridTextColumn Header="时间" Width="200" Binding="{Binding time}"/>
                            <DataGridTextColumn Header="状态" Width="100" Binding="{Binding status}"/>
                        </DataGrid.Columns>
                    </DataGrid>
                </Grid>
            </TabItem>
            <TabItem Header="设备状态">
                <Grid Name="status_show" Background="#FFE5E5E5">
                    <TreeView Margin="10,50,0,0" Name="tv_status1" HorizontalAlignment="Left" VerticalAlignment="Top" Width="200" Height="660">
                    </TreeView>
                    <TreeView Margin="232,50,0,0" Name="tv_status2" HorizontalAlignment="Left" VerticalAlignment="Top" Width="200" Height="660">
                    </TreeView>
                    <TreeView Margin="450,50,0,0" Name="tv_status3" HorizontalAlignment="Left" VerticalAlignment="Top" Width="200" Height="660">
                    </TreeView>
                    <TreeView Margin="670,50,0,0" Name="tv_status4" HorizontalAlignment="Left" VerticalAlignment="Top" Width="200" Height="660"/>
                    <Label x:Name="label" Content="控制端状态：" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
                    <Ellipse Name="ell_moniter" Fill="Blue" HorizontalAlignment="Left" Height="32" Margin="97,7,0,0" Stroke="Black" VerticalAlignment="Top" Width="32"/>
                </Grid>
            </TabItem>
            <TabItem Header="账号管理">
